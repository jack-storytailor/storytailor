    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "lib": ["es2016"],
    "outDir": "../storyscript-ts-plugin/node_modules/storyscript/out",
    "rootDir": "./src",
    "declaration": true
