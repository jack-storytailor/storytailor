{
  "compilerOptions": {
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "lib": [ "es2016" ],
    "outDir": "./out",
    "rootDir": "./src",
    "declaration": true
  },
  "exclude": [
    "node_modules",
    "out"
  ]
}